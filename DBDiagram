Table EGON_Data {
  Id integer [primary key]
  Temperature decimal
  Humidity decimal
  LocationID integer
  SQLTStamp timestamp 
}

Table EGON_Location {
  LocationID integer [primary key]
  Room nvarchar(50)
  Floor integer
  Building nvarchar(50)
}


Table EGON_Power {
  Id integer [primary key]
  KW_YearSummarized decimal
  KW_Day decimal
  WattHour decimal
  WattPeakDay decimal
  LocationID integer
  SQLTStamp timestamp
}
Ref: EGON_Data.LocationID > EGON_Location.LocationID
Ref: EGON_Power.LocationID > EGON_Location.LocationID
